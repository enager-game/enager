{
  "files.associations": {
    "*.css": "postcss"
  },
  "files.exclude": {
    "package/node_modules/": true,
    "package/.next/": true,
    "/build": true,
    ".DS_Store": true,
    "**/*.log": true,
    ".env*.local": true,
    "*.tsbuildinfo": true
  },
  "emmet.includeLanguages": {
    "postcss": "css",
    "typescriptreact": "javascriptreact"
  },
  "javascript.suggest.paths": false,
  "typescript.suggest.paths": false
}
